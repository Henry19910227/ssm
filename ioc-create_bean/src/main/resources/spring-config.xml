<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user" class="com.henry.model.com.henry.model.User"/>

    <bean id="user2" class="com.henry.model.User2" factory-method="createUser"/>

    <bean id="user3" class="com.henry.model.User3"/>
    <bean id="user3-factory" factory-bean="user3" factory-method="createUser"/>

</beans>